from kqcircuits.klayout_view import KLayoutView
from kqcircuits.simulations.simulation import Simulation
from kqcircuits.simulations.export.hfss.hfss_export import HfssExport
from kqcircuits.defaults import TMP_PATH

"""Before using this script, have your project in view as the top cell."""

from pathlib import Path
path = TMP_PATH # Your path here
name = "Dev" # makes filenames Dev.oas, Dev.gds and Dev.json 

top_cell = KLayoutView.get_active_cell()

sim = Simulation.from_cell(top_cell, margin=100, name=name)

hfss = HfssExport(sim, path=path)
hfss.write()

print("Exported simulation to {}".format(path))</text>
